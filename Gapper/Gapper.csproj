<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <AssemblyName>Gapper</AssemblyName>
    <RootNamespace>Gapper</RootNamespace>
    <Version>1.1.2</Version>
    <Authors>Viktor Nilsson</Authors>
    <Company>Viktor Nilsson</Company>
    <Copyright>Copyright (c) 2017 Viktor Nilsson</Copyright>
    <PackageLicenseUrl>https://gitlab.com/viktornilsson91/gapper/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://gitlab.com/viktornilsson91/gapper</PackageProjectUrl>
    <RepositoryUrl>https://gitlab.com/viktornilsson91/gapper</RepositoryUrl>
    <PackageTags>dapper extension crud gapper</PackageTags>
    <PackageId>Gapper</PackageId>
    <Product>Gapper</Product>
    <Description>Dapper extension with simple crud methods.</Description>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageIconUrl>https://gitlab.com/viktornilsson91/gapper/raw/master/gapper-logo.png</PackageIconUrl>
    <ApplicationIcon />
    <Win32Resource />
    <DebugType>portable</DebugType>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|netstandard1.6|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard1.6\Gapper.xml</DocumentationFile>
    <NoWarn>1701;1702;1705;1591</NoWarn>    
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="1.50.5" />
  </ItemGroup>

</Project>